# Free NAS mode: no external backend required.
# Data persistence is local-only in browser storage.

# Optional S3-compatible storage
AWS_REGION=eu-west-3
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=

# Auth providers are disabled in free mode
PUBLIC_ENABLE_DISCORD_AUTH=false
PUBLIC_ENABLE_TWITCH_AUTH=false
PUBLIC_ENABLE_GOOGLE_AUTH=false

PUBLIC_SHOW_FULL_LOCATION_DETAILS=false
PUBLIC_OAUTH_REDIRECT_URL=http://127.0.0.1:4173
